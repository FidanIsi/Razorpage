    <script src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

    <script>
        $(document).ready(function () {
            var sliderFor = $('.slider-for');
            var sliderNav = $('.slider-nav');

            function initSliders() {
                $('.slider-for').slick({
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    arrows: false,
                    fade: true,
                    asNavFor: '.slider-nav'
                });

                $('.slider-nav').slick({
                    slidesToShow: 5,
                    slidesToScroll: 1,
                    asNavFor: '.slider-for',
                    dots: false,
                    focusOnSelect: true
                });

                function setOpacity() {
                    sliderNav.find('.slick-slide').css('opacity', 0.5);
                    sliderNav.find('.slick-current').css('opacity', 1);
                }

                setOpacity();

                sliderFor.on('beforeChange', function (event, slick, currentSlide, nextSlide) {
                    sliderNav.find('.slick-slide').css('opacity', 0.5);
                    sliderNav.find('.slick-slide[data-slick-index="' + nextSlide + '"]').css('opacity', 1);
                });
            }

            $('#details').on('shown.bs.modal', function () {
                initSliders();
            });
        });
    </script>
    <script>
        AOS.init();
    </script>

